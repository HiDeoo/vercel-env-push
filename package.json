  "devDependencies": {
    "@hideoo/eslint-config": "0.0.4",
    "@hideoo/prettier-config": "0.0.1",
    "@types/node": "14.18.21",
    "eslint": "8.18.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "typescript": "4.7.4",
    "unbuild": "0.7.4",
    "vitest": "0.17.0"
  },
  "packageManager": "pnpm@7.4.0",
  "engines": {
    "node": ">=14.0.0"
  },
  "prettier": "@hideoo/prettier-config",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown --cache",
